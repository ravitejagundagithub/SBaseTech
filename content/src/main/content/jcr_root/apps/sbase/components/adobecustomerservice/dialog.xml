<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="adobecustomerservice"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <connect
                jcr:primaryType="cq:Panel"
                title="Connect">
                <items jcr:primaryType="cq:WidgetCollection">
                    <title
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Title"
                        name="./connectTitle"
                        xtype="textfield"/>
                </items>
            </connect>
            <contact
                jcr:primaryType="cq:Panel"
                title="Contact">
                <items jcr:primaryType="cq:WidgetCollection">
                    <title
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Title"
                        name="./contactTitle"
                        xtype="textfield"/>
                    <desc
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Description"
                        name="./contactDesc"
                        xtype="textfield"/>
                    <phone
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Phone Number"
                        name="./phone"
                        xtype="textfield"/>
                </items>
            </contact>
            <request
                jcr:primaryType="cq:Panel"
                title="Request">
                <items jcr:primaryType="cq:WidgetCollection">
                    <title
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Title"
                        name="./requestTitle"
                        xtype="textfield"/>
                    <desc
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Description"
                        name="./requestDesc"
                        xtype="textfield"/>
                    <buttontext
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Button Text"
                        name="./buttonText"
                        xtype="textfield"/>
                </items>
            </request>
        </items>
    </items>
</jcr:root>
